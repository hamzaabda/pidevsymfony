<body>

<main>
    <div class="container">

        <section class="section register min-vh-100 d-flex flex-column align-items-center justify-content-center py-4">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-4 col-md-6 d-flex flex-column align-items-center justify-content-center">

                        <div class="d-flex justify-content-center py-4">
                            <a href="index.html" class="logo d-flex align-items-center w-auto">
                                <img src="assets/img/logo.png" alt="">
                                <span class="d-none d-lg-block">Inscription Citoyen</span>
                            </a>
                        </div><!-- End Logo -->

                        <div class="card mb-3">

                            <div class="card-body">

                                <div class="pt-4 pb-2">
                                    <h5 class="card-title text-center pb-0 fs-4">Créer Une Compte Citoyen</h5>
                                    <p class="text-center small">Entrer vos données peronnels pour créer une compte</p>
                                </div>

                                {{ form_start(registrationForm,{'attr':{'novalidate': 'novalidate'}})  }}


                                <label for="inputNanme4" class="form-label">Nom :</label>
                                {{ form_widget(registrationForm.nom,{'attr':{'class':'form-control'}}) }}
                                <span style="color:red">{{ form_errors(registrationForm.nom) }}</span>


                                <label for="inputNanme4" class="form-label">Prénom :</label>
                                {{ form_widget(registrationForm.prenom,{'attr':{'class':'form-control'}}) }}
                                <span style="color:red">{{ form_errors(registrationForm.prenom) }}</span>


                                <label for="inputEmail4" class="form-label">Email :</label>
                                {{ form_widget(registrationForm.email,{'attr':{'class':'form-control'}}) }}
                                <span style="color:red">{{ form_errors(registrationForm.email) }}</span>

                                <label for="inputNanme4" class="form-label">CIN :</label>
                                {{ form_widget(registrationForm.cin,{'attr':{'class':'form-control'}}) }}
                                <span style="color:red">{{ form_errors(registrationForm.cin) }}</span>



                                <label for="inputPassword4" class="form-label">Mot De Passe :</label>
                                {{ form_widget(registrationForm.motDePasse,{'attr':{'class':'form-control'}}) }}
                                <span style="color:red">{{ form_errors(registrationForm.motDePasse) }}</span>


                                <label for="inputPassword4" class="form-label">Confirmer Mot De Passe :</label>
                                {{ form_widget(registrationForm.confirmMotDePasse,{'attr':{'class':'form-control'}}) }}
                                <span style="color:red">{{ form_errors(registrationForm.confirmMotDePasse) }}</span>
                                <span class="txt1 p-b-11"> êtes vous un robot ?</span>
                                <div class="wrap-input100 m-b-36">
                                    {{ form_widget(registrationForm.captchaCode, {'attr': {'class': 'input100'} }) }}
                                    <span class="focus-input100"></span>
                                </div>

                                <br/>
                                <div class="text-center">
                                    {{ form_widget(registrationForm.ajouter,{'attr':{'class':'btn btn-primary'}})}}
                                    <button type="reset" class="btn btn-secondary">Annuler</button>
                                </div>
                                <br/>

                                <div class="error">{{ form_errors(registrationForm.captchaCode) }} </div>
                                {{ form_end(registrationForm) }}<!-- Vertical Form -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section>

    </div>
</main><!-- End #main -->

